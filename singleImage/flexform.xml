<T3DataStructure>
	<ROOT>
		<type>array</type>
			<el>
				<link>
					<TCEforms type="array">
						<config type="array">
							<type>input</type>
							<size>15</size>
							<max>256</max>
							<checkbox></checkbox>
							<eval>trim</eval>
							<wizards type="array">
								<_PADDING type="integer">2</_PADDING>
								<link type="array">
									<type>popup</type>
									<title>Link</title>
									<icon>link_popup.gif</icon>
									<script>browse_links.php?mode=wizard</script>
									<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
								</link>
							</wizards>
						</config>
						<label>Page Link (or YouTube ID if video)</label>
					</TCEforms>
				</link>
				<image>
					<TCEforms type="array">
						<config type="array">
							<type>group</type>
							<internal_type>file</internal_type>
							<allowed>gif,png,jpg,jpeg</allowed>
							<max_size>5000</max_size>
							<uploadfolder>uploads/tx_sotocontentelements/imagePromo</uploadfolder>
							<show_thumbs>1</show_thumbs>
							<size>1</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
						</config>
						<label>Image</label>
					</TCEforms>
				</image>
				<imageAspectRatio>
					<TCEforms>
						<label>Image Aspect Ratio</label>
						<config>
							<type>select</type>
							<items type="array">
								<numIndex index="1" type="array">
									<numIndex index="0">5:1</numIndex>
									<numIndex index="1">5</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="0">4:1</numIndex>
									<numIndex index="1">4</numIndex>
								</numIndex>
								<numIndex index="3" type="array">
									<numIndex index="0">3:1 (Standard TV)</numIndex>
									<numIndex index="1">3</numIndex>
								</numIndex>
								<numIndex index="4" type="array">
									<numIndex index="0">2:1</numIndex>
									<numIndex index="1">2</numIndex>
								</numIndex>
								<numIndex index="5" type="array">
									<numIndex index="0">1:1</numIndex>
									<numIndex index="1">1</numIndex>
								</numIndex>
								<numIndex index="6" type="array">
									<numIndex index="0">4:3 (Standard TV)</numIndex>
									<numIndex index="1">43</numIndex>
								</numIndex>
								<numIndex index="7" type="array">
									<numIndex index="0">16:9 (Widescreen HD TV)</numIndex>
									<numIndex index="1">169</numIndex>
								</numIndex>
								<numIndex index="8" type="array">
									<numIndex index="0">3:2 (35mm)</numIndex>
									<numIndex index="1">32</numIndex>
								</numIndex>
								<numIndex index="9" type="array">
									<numIndex index="0">Keep original proportion</numIndex>
									<numIndex index="1">0</numIndex>
								</numIndex>
							</items>
							<default>0</default>
						</config>
					</TCEforms>
				</imageAspectRatio>
				<maxWidth type="array">
					<TCEforms type="array">
						<config type="array">
							<type>input</type>
							<size>2</size>
							<max>0</max>
							<eval>int</eval>
							<range type="array">
								<upper>2000</upper>
								<lower>0</lower>
							</range>
							<default type="integer">0</default>
						</config>
						<label>Max Width</label>
					</TCEforms>
				</maxWidth>
				<maxWidthUnit>
					<TCEforms>
						<label>Max Width Unit (pixels or percentage)</label>
						<config>
							<type>select</type>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="0">Pixels</numIndex>
									<numIndex index="1">px</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="0">Percentage</numIndex>
									<numIndex index="1">%</numIndex>
								</numIndex>
							</items>
							<default>px</default>
						</config>
					</TCEforms>
				</maxWidthUnit>
				<cssClass type="array">
					<TCEforms type="array">
						<config type="array">
							<type>input</type>
							<size>48</size>
							<eval>trim</eval>
							<default>0,0,0,0</default>
						</config>
						<label>CSS Class(es)</label>
					</TCEforms>
				</cssClass>
				<margin type="array">
					<TCEforms type="array">
						<config type="array">
							<type>input</type>
							<size>48</size>
							<eval>trim</eval>
							<default>0,0,0,0</default>
						</config>
						<label>Margin (t,r,b,l)</label>
					</TCEforms>
				</margin>
				<padding type="array">
					<TCEforms type="array">
						<config type="array">
							<type>input</type>
							<size>48</size>
							<eval>trim</eval>
							<default>0,0,0,0</default>
						</config>
						<label>Padding (t,r,b,l)</label>
					</TCEforms>
				</padding>
				<borderWidth type="array">
					<TCEforms type="array">
						<config type="array">
							<type>input</type>
							<size>48</size>
							<eval>trim</eval>
							<default>0</default>
						</config>
						<label>Border Width (leave blank for none)</label>
					</TCEforms>
				</borderWidth>
				<borderColor type="array">
					<TCEforms type="array">
						<config type="array">
							<type>input</type>
							<size>48</size>
							<eval>trim</eval>
							<default>0</default>
						</config>
						<label>Border Color</label>
					</TCEforms>
				</borderColor>
				<borderStyle>
					<TCEforms>
						<label>Image Aspect Ratio</label>
						<config>
							<type>select</type>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="0">Solid</numIndex>
									<numIndex index="1">solid</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="0">Dotted</numIndex>
									<numIndex index="1">dotted</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="0">Dashed</numIndex>
									<numIndex index="1">dashed</numIndex>
								</numIndex>
								<numIndex index="3" type="array">
									<numIndex index="0">Double</numIndex>
									<numIndex index="1">double</numIndex>
								</numIndex>
								<numIndex index="4" type="array">
									<numIndex index="0">Groove</numIndex>
									<numIndex index="1">groove</numIndex>
								</numIndex>
								<numIndex index="5" type="array">
									<numIndex index="0">Ridge</numIndex>
									<numIndex index="1">ridge</numIndex>
								</numIndex>
								<numIndex index="6" type="array">
								 	<numIndex index="0">Inset</numIndex>
								 	<numIndex index="1">inset</numIndex>
								</numIndex>
								<numIndex index="6" type="array">
									<numIndex index="0">Outset</numIndex>
									<numIndex index="1">outset</numIndex>
								</numIndex>
							</items>
							<default>solid</default>
						</config>
					</TCEforms>
				</borderStyle>
				<display>
					<TCEforms>
						<label>Image Aspect Ratio</label>
						<config>
							<type>select</type>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="0">Block</numIndex>
									<numIndex index="1">display:block</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="0">Inline</numIndex>
									<numIndex index="1">display:inline</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="0">Float Left</numIndex>
									<numIndex index="1">float:left</numIndex>
								</numIndex>
								<numIndex index="3" type="array">
									<numIndex index="0">Float Right</numIndex>
									<numIndex index="1">float:right</numIndex>
								</numIndex>
							</items>
							<default>display:block</default>
						</config>
					</TCEforms>
				</display>
				<position>
					<TCEforms>
						<label>Image Aspect Ratio</label>
						<config>
							<type>select</type>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="0">Static</numIndex>
									<numIndex index="1">position:static</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="0">Absolute</numIndex>
									<numIndex index="1">position:absolute</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="0">Relative</numIndex>
									<numIndex index="1">position:relative</numIndex>
								</numIndex>
							</items>
							<default>position:static</default>
						</config>
					</TCEforms>
				</position>
				<positionCoordinatesOrigin>
					<TCEforms>
						<label>Image Aspect Ratio</label>
						<config>
							<type>select</type>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="0">Static</numIndex>
									<numIndex index="1">position:static</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="0">Absolute</numIndex>
									<numIndex index="1">position:absolute</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="0">Relative</numIndex>
									<numIndex index="1">position:relative</numIndex>
								</numIndex>
							</items>
							<default>position:static</default>
						</config>
					</TCEforms>
				</positionCoordinatesOrigin>
				<zIndex type="array">
					<TCEforms type="array">
						<config type="array">
							<type>input</type>
							<size>4</size>
							<eval>trim</eval>
						</config>
						<label>Z-Index</label>
					</TCEforms>
				</zIndex>
			</el>
	</ROOT>
</T3DataStructure>